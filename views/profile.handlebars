 {{>navbar   active_profile=true}}
<div class="container">
        <div class="row signupBox text-center">
            <div class="col-md-12 text-center">
                <h2><span>Your Reviews Reviews</span></h2>
            </div>
        </div>
        <div class="row">
            {{#each Reviews}}
    <div class="card bg-light mb-3 mx-auto" style="max-width: 18rem;">
        <h4 class="card-header d-flex align-items-center justify-content-center text-center h-100">{{this.title}}</h4>
        <div class="card-body flex-column h-100">
            {{!--  --}}
          <h6 class="card-title">By {{this.authorName}}</h6>
          <p class="card-text">"{{this.review}}"</p>
          <p class="card-text text-right">Created on {{this.createdAt}}</p>
        </div>
      </div>
      {{/each}}
      </div>
    </div>